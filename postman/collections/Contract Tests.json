							"    });",
							"}",
							"else {",
							"    pm.request.headers.insert({ \"key\": \"x-mock-response-name\",",
							"                                \"value\": \"Tests Pass\" ",
							"    });",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
